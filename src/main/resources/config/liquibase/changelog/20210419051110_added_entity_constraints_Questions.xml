        <addForeignKeyConstraint baseColumnNames="questions_id"
                                 baseTableName="rel_questions__tags"
                                 constraintName="fk_rel_questions__tags__questions_id"
                                 referencedColumnNames="id"
                                 referencedTableName="questions"/>

        <addForeignKeyConstraint baseColumnNames="tags_id"
                                 baseTableName="rel_questions__tags"
                                 constraintName="fk_rel_questions__tags__tags_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tags"/>

        <addForeignKeyConstraint baseColumnNames="users_id"
                                 baseTableName="questions"
                                 constraintName="fk_questions__users_id"
